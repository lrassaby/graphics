<scenefile>
	<globaldata>
		<diffusecoeff v="0.5"/>
		<specularcoeff v="0.5"/>
		<ambientcoeff v="0.5"/>
	</globaldata>

    <lightdata>
	<id v="0"/>
	<color r="1" g="1" b="1"/>
	<position x="4" y="4" z="4"/>
    </lightdata>

    <lightdata>
	<id v="1"/>
	<color r="1" g="1" b="1"/>
	<position x="-3" y="3" z="3"/>
    </lightdata>

    <lightdata>
	<id v="2"/>
	<color r="1" g="1" b="1"/>
	<position x="-3" y="3" z="-3"/>
    </lightdata>

    <lightdata>
	<id v="3"/>
	<color r="1" g="1" b="1"/>
	<position x="3" y="3" z="-3"/>
    </lightdata>

    <object type="tree" name="root">
        <transblock>
        <rotate x="0" y="1" z="0" angle="0"/>
        /* head */
        <object type="tree" name="head">
            <transblock>
                <scale x="1.2" y="1.4" z="1.2"/>
                <translate x="0" y="1.8" z="0"/>
                <object type="primitive" name="sphere" >
                    <diffuse r="1.0" g="0.86" b="0.7"/>
                </object>
            </transblock>
        </object>
         /* eye */
        <object type="tree" name="eye">
            <transblock>
                <scale x=".15" y=".15" z=".15"/>
                <translate x="-1.3" y="19" z="3.5"/>
                <object type="primitive" name="sphere" >
                    <diffuse r="0" g="0.86" b="1.0"/>
                </object>
            </transblock>
        </object>
        /* eye */
        <object type="tree" name="eye">
            <transblock>
                <scale x=".15" y=".15" z=".15"/>
                <translate x="1.3" y="19" z="3.5"/>
                <object type="primitive" name="sphere" >
                    <diffuse r="0" g="0.86" b="1.0"/>
                </object>
            </transblock>
        </object>
     /* mouth */
        <object type="tree" name="mouth">
            <transblock>
                <scale x=".3" y=".3" z=".3"/>
                <translate x="0" y="9" z="2"/>
                <object type="primitive" name="cone" >
                    <diffuse r="1.0" g="0.86" b="0.7"/>
                </object>
            </transblock>
        </object>


        /* body */
        <object type="tree" name="root">
            <transblock>
                <scale x="1.5" y="4.0" z="1.5"/>
                <object type="primitive" name="sphere" >
                    <diffuse r="0" g="0.9" b="0.2"/>
                </object>
            </transblock>
        </object>

        /* leg */
        <object type="tree" name="root">
            <transblock>
                <translate x="-0.5" y="-2.0" z="0"/>
                <rotate x="1" y="0" z="0" angle="180"/>
                <rotate x="0" y="0" z="1" angle="10"/>
                <scale x=".8" y="5.0" z=".8"/>
                <object type="primitive" name="cylinder" >
                    <diffuse r="0" g="0.9" b="0.9"/>
                </object>
            </transblock>
        </object>

        /* leg */
        <object type="tree" name="root">
            <transblock>
                <translate x="0.3" y="-2.0" z="0"/>
                <rotate x="1" y="0" z="0" angle="180"/>
                <rotate x="0" y="0" z="1" angle="-10"/>
                <scale x=".8" y="5.0" z=".8"/>
                <object type="primitive" name="cylinder" >
                    <diffuse r="0" g="0.9" b="0.9"/>
                </object>
            </transblock>
        </object>

        /* arm */
        <object type="tree" name="arm">
            <transblock>
                <translate x="-0.5" y="1.0" z="0.2"/>
                <rotate x="0" y="0" z="1" angle="90"/>
                <rotate x="0" y="1" z="1" angle="80"/>
                <scale x="0.5" y="2.0" z="0.3"/>
                <object type="primitive" name="cylinder" >
                    <diffuse r="1.0" g="0.3" b="0"/>
                </object>
            </transblock>
        </object>
        /* arm after elbow */
        <object type="tree" name="forearm">
            <transblock>
                <translate x="-1.3" y="0.3" z="1"/>
                <rotate x="0" y="1" z="0" angle="60"/>
                <rotate x="0" y="0" z="1" angle="90"/>
                <scale x="0.4" y="1.2" z="0.25"/>
                <object type="primitive" name="cylinder" >
                    <diffuse r=".9" g="0.86" b="0.7"/>
                </object>
            </transblock>
        </object>

        /* arm */
        <object type="tree" name="arm">
            <transblock>
                <translate x="0.5" y="1.0" z="0.2"/>
                <rotate x="0" y="0" z="1" angle="-90"/>
                <rotate x="0" y="1" z="1" angle="-80"/>
                <scale x="0.5" y="2.0" z="0.3"/>
                <object type="primitive" name="cylinder" >
                    <diffuse r="1.0" g="0.3" b="0"/>
                </object>
            </transblock>
        </object>

        /* arm after elbow */
        <object type="tree" name="forearm">
            <transblock>
                <translate x="1.3" y="0.3" z="1"/>
                <rotate x="0" y="1" z="0" angle="-60"/>
                <rotate x="0" y="0" z="1" angle="-90"/>
                <scale x="0.4" y="1.2" z="0.25"/>
                <object type="primitive" name="cylinder" >
                    <diffuse r="1.0" g="0.86" b="0.7"/>
                </object>
            </transblock>
        </object>
        
        /* staff */
        <object type="tree" name="staff">
            <transblock>
                <translate x="-1.45" y="-.4" z="1.5"/>
                <rotate x="0" y="1" z="0" angle="90"/>
                <scale x="0.1" y="7" z="0.1"/>
                <object type="primitive" name="cylinder" >
                    <diffuse r="0.1" g="0.3" b="0.6"/>
                </object>
            </transblock>
        </object>
 


    </transblock>
    </object>
</scenefile>

